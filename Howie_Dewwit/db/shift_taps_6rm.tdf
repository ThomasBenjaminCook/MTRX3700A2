FUNCTION altsyncram_kn81 (address_a[9..0], address_b[9..0], clock0, clocken0, data_a[51..0], wren_a)
RETURNS ( q_b[51..0]);
FUNCTION cntr_vtf (clk_en, clock)
RETURNS ( q[9..0]);

--synthesis_resources = M9K 6 
SUBDESIGN shift_taps_6rm
( 
	clken	:	input;
	clock	:	input;
	shiftin[51..0]	:	input;
	shiftout[51..0]	:	output;
	taps[51..0]	:	output;
) 
VARIABLE 
	altsyncram2 : altsyncram_kn81;
	cntr1 : cntr_vtf;

BEGIN 
	altsyncram2.address_a[] = cntr1.q[];
	altsyncram2.address_b[] = cntr1.q[];
	altsyncram2.clock0 = clock;
	altsyncram2.clocken0 = clken;
	altsyncram2.data_a[] = ( shiftin[]);
	altsyncram2.wren_a = B"1";
	cntr1.clk_en = clken;
	cntr1.clock = clock;
	shiftout[51..0] = altsyncram2.q_b[51..0];
	taps[] = altsyncram2.q_b[];
END;
